{
  "private": true,
  "name": "now-examples-ghost",
  "description": "Example deployment of a `ghost` blog to Now using npm",
  "scripts": {
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/now-examples/ghost.git"
  },
  "keywords": [
    "now",
    "deploy",
    "zeit",
    "ghost",
    "blog"
  ],
  "author": "Nathan Rajlich <nate@zeit.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/now-examples/ghost/issues"
  },
  "homepage": "https://github.com/now-examples/ghost#readme",
  "engines": {
    "ace": "beta"
  },
  "dependencies": {
    "ghost": "0.11.4"
  },
  "now": {
    "alias": "now-examples-ghost.now.sh",
    "env": {
      "GHOST_NODE_VERSION_CHECK": "false",
      "NODE_ENV": "production"
    }
  }
}
